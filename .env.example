# Copy this file to .env and fill in your Supabase project values

# Required
SUPABASE_URL=https://<your-project-ref>.supabase.co
SUPABASE_ANON_KEY=<your-anon-or-publishable-key>
# Or (preferred for server-side scripts)
# SUPABASE_SERVICE_ROLE_KEY=<your-service-role-key>

# Optional
SUPABASE_TABLE=a_dis

# API key used by RLS policy (sent as request header x-bubble-key)
BUBBLE_API_KEY=123456

# Monitor options
POLL_MINUTES=10
EXCEL_PATH=list.xlsx
SHEET_NAME=Sheet1
# Run once immediately on start (1/0)
RUN_ON_START=1
